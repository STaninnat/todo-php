DB_PORT=YOUR_PORT

LC_DB_HOST="YOUR_HOST"
LC_DB_NAME="YOUR_NAME"
LC_DB_USER="YOUR_USER"
LC_DB_PASS="YOUR_PASS"
LC_DB_PORT=YOUR_PORT

MYSQL_ROOT_PASSWORD="YOUR_PASS"
MYSQL_DATABASE="YOUR_DB"
MYSQL_USER="YOUR_NAME"
MYSQL_PASSWORD="YOUR_PASS"
PHPMYADMIN_PORT=YOUR_PORT
PHPMYADMIN_UPLOAD_LIMIT="128M"

JWT_SECRET="YOUR_SECRET"